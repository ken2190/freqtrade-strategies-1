{
    "dry_run": true,
    "forcebuy_enable": false,
    "max_open_trades": 5,
    "stake_currency": "USDT",
    "stake_amount": "unlimited",
    // "available_capital": 1000,
    "tradable_balance_ratio": 0.99,
    "fiat_display_currency": "USD",
    "timeframe": "5m",
    "cancel_open_orders_on_exit": false,
    "unfilledtimeout": {
        "buy": 10,
        "sell": 30,
        "unit": "minutes"
    },
  "exchange": {
        "name": "binance",
        "key": "",
        "secret": "",
        "ccxt_config": {},
        "ccxt_async_config": {},
        "pair_whitelist": [
        ],
        "pair_blacklist": [
            "BNB/.*",
            ".*(AUD|BRZ|CAD|CHF|DAI|EUR|GBP|HKD|IDRT|JPY|NGN|PAX|PAXG|RUB|SGD|SUSD|TRY|TUSD|UAH|USD|USDC|VAI|ZAR)/.*",
            ".*(_PREMIUM|BEAR|BULL|DOWN|HALF|HEDGE|UP|[1235][SL]).*/.*",
    
        ]
    },

    "_pairlists": [
        {"method": "StaticPairList"}
    ],
    "pairlists": [
            {
                "method": "VolumePairList",
                "number_assets": 80,
                "sort_key": "quoteVolume",
                "refresh_period": 1800
            },
            {"method": "AgeFilter", "min_days_listed": 3},
            {"method": "SpreadFilter", "max_spread_ratio": 0.005},
            {"method": "PriceFilter", "min_price": 0.0001},
            {
                "method": "RangeStabilityFilter",
                "lookback_days": 3,
                "min_rate_of_change": 0.05,
                "refresh_period": 1440
            },
            {
                "method": "VolatilityFilter",
                "lookback_days": 3,
                "min_volatility": 0.01,
                "max_volatility": 0.75,
                "refresh_period": 43400
            },
            {
                 "method": "PerformanceFilter",
                 "minutes": 1440,
                 "min_profit": 0.01
            }


    ],

    "initial_state": "running",
    "internals": {
        "process_throttle_secs": 5,
        
    }
}
